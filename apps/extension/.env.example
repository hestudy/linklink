# Extension应用环境变量

# 扩展配置
EXTENSION_ID="your-extension-id"
EXTENSION_NAME="LinkLink AI Bookmark Manager"
EXTENSION_VERSION="0.0.0"

# 服务器配置
VITE_SERVER_URL="http://localhost:3000"
VITE_WEB_URL="http://localhost:3001"

# 认证配置
VITE_BETTER_AUTH_URL="http://localhost:3001"

# 开发环境配置
NODE_ENV="development"
LOG_LEVEL="debug"

# AI服务配置（预留）
OPENAI_API_KEY=""
OPENAI_ORG_ID=""

# 分析和监控
ENABLE_ANALYTICS="false"
ENABLE_DEBUG="true"

# 浏览器兼容性
TARGET_BROWSER="chrome"
MANIFEST_VERSION="manifest.json"